{
  "name": "rutgers-design2b-2017",
  "version": "1.0.0",
  "description": "Class site for Rugers Design II-B, Spring 2017",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch\" \"npm run server\"",
    "highlight": "node node_modules/highlight.js/tools/build.js -t browser",
    "build": "node index.js",
    "watch": "node index.js --watch",
    "server": "http-server build/",
    "deploy-gh": "npm run build; gh-pages -d build",
    "deploy": "npm run build; rsync -avz build/ /Volumes/art.rutgers.edu/dayton/Sites/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyinabox/rutgers-design2b-2017.git"
  },
  "author": "Andy Dayton <amdayton@gmail.com> (http://andydayton.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andyinabox/rutgers-design2b-2017/issues"
  },
  "homepage": "https://github.com/andyinabox/rutgers-design2b-2017#readme",
  "dependencies": {
    "dom-css-transform": "^2.0.2",
    "google-fonts": "^1.0.0",
    "highlight.js": "^9.9.0",
    "metalsmith-build-date": "andyinabox/metalsmith-build-date#timestamp",
    "mouse-position": "^2.0.1",
    "smooth-scroll": "cferdinandi/smooth-scroll"
  },
  "devDependencies": {
    "browserify": "^13.3.0",
    "concurrently": "^3.1.0",
    "gh-pages": "^0.12.0",
    "handlebars": "^4.0.6",
    "metalsmith": "^2.3.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-browserify": "^0.2.4",
    "metalsmith-layouts": "^1.7.0",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-rootpath": "^1.0.4",
    "metalsmith-sass": "^1.4.0",
    "metalsmith-watch": "^1.0.3",
    "minimist": "^1.2.0",
    "watchify": "^3.8.0"
  }
}
