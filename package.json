  "dependencies": {
    "browserify": "^13.3.0",
    "concurrently": "^3.1.0",
    "metalsmith": "^2.3.0",
    "metalsmith-browserify": "^0.2.4",
    "metalsmith-layouts": "^1.7.0",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-metallic": "^2.0.1",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-sass": "^1.4.0",
    "metalsmith-watch": "^1.0.3",
